<Window x:Class="Спартак_Диплом.TableSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Спартак_Диплом"
        mc:Ignorable="d"
        Title="Выбор таблицы" Height="360" Width="540" MinWidth="540" MinHeight="360">
    <Grid>
        <Button x:Name="ButtonOpenOrders" Content="Заказы" Margin="50,50,50,0" VerticalAlignment="Top" Height="50" Click="OpenOrdersButton_Click"/>
        <Button x:Name="ButtonOpenCliens" Content="Клиенты" Margin="50,125,50,0" VerticalAlignment="Top" Height="50" Click="ButtonOpenCliens_Click"/>
        <Button x:Name="ButtonOpenProducts" Content="Товары" Margin="50,200,50,0" VerticalAlignment="Top" Height="50" Click="ButtonOpenProducts_Click"/>

    </Grid>
</Window>
